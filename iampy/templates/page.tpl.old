%setdefault("page_title", "DocType")
%rebase("base.tpl")
<div class="wrapper">
    <aside class="navbar navbar-vertical navbar-expand-lg navbar-dark"></aside>
    <div class="page-wrapper">
        <div class="container-fluid">
            <div class="page-header d-print-none">
                <div class="row align-items-center">
                    <div class="col">
                        % if defined("page_pre_title"):
                        <div class="page-pretitle">{{ page_pre_title }}</div>
                        % end
                        <h2 class="page-title">{{ page_title }}</h2>
                    </div>
                    <div class="col-auto ms-auto d-print-none">
                        <div class="btn-list">
                            <a class="btn btn-primary sm-inline-block btn-icon">
                                <i class="ti ti-plus"></i> Create new DocType
                            </a>
                            <a class="btn btn-primary d-sm-none btn-icon">
                                <i class="ti ti-plus"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="page-body">
            <div class="container-fluid">
                <div class="row row-deck row-cards">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-header">
                                <ul class="nav nav-pills card-header-pills">
                                    <li class="nav-item"><a class="nav-link" aria-label="Create new DocType"><i class="ti ti-plus"></i></a></li>
                                    <li class="nav-item"><a class="nav-link" aria-label="Edit selected DocType"><i class="ti ti-pencil"></i></a></li>
                                    <li class="nav-item"><a class="nav-link" aria-label="Suspended selected DocType"><i class="ti ti-arrow-back-up"></i></a></li>
                                    <li class="nav-item"><a class="nav-link" aria-label="Restore selected DocType"><i class="ti ti-arrow-forward-up"></i></a></li>
                                    <li class="nav-item"><a class="nav-link" aria-label="Filter DocType"><i class="ti ti-filter"></i></a></li>
                                    <li class="nav-item">
                                         <div class="my-2 my-md-0 flex-grow-1 flex-md-grow-0" style="margin-top: 4px !important">
                                            <form action="." method="get">
                                                <div class="input-icon">
                                                    <input type="text" class="form-control form-control-sm" placeholder="Search..." aria-label="Search in DocType">
                                                    <span class="input-icon-addon">
                                                        <i class="ti ti-search"></i>
                                                    </span>
                                                </div>
                                            </form>
                                        </div>
                                    </li>
                                    <li class="nav-item">
                                        <div class="my-2 my-md-0 flex-grow-1 flex-md-grow-0" style="margin-top: 4px !important">
                                            <form action="." method="get">
                                                <select type="text" class="form-select form-select-sm" id="filters" value="">
                                                    <option value="active" selected>Active</option>
                                                    <option value="inactive">Inactive</option>
                                                </select>
                                            </form>
                                        </div>
                                    </li>
                                    <li class="nav-item"><a class="nav-link" aria-label="Reset all Filters on DocType"><i class="ti ti-filter-off"></i></a></li>
                                    <li class="nav-item"><a class="nav-link" aria-label="Refresh table of DocType"><i class="ti ti-refresh"></i></a></li>
                                    <li class="nav-item"><a class="nav-link" aria-label="Export to Excel"><i class="ti ti-download"></i></a></li>
                                </ul>
                            </div>
                            <div class="table-responsive">
                                <table class="table card-table table-vcenter table-nowrap">
                                    <thead>
                                        <tr>
                                            <th class="w-1">
                                                <input class="form-check-input m-0 align-middle" type="checkbox" aria-label="Select all DocTypes">
                                            </th>
                                            <th class="w-1">No. <i class="ti ti-chevron-up"></i></th>
                                            <th>Name</th>
                                            <th>Label</th>
                                            <th>Is Single</th>
                                            <th>Is Child</th>
                                            <th>Is Tree</th>
                                            <th>Is Submittable</th>
                                            <th>&nbsp;</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td class="w-1">
                                                <input class="form-check-input m-0 align-middle" type="checkbox" aria-label="Select DocType FieldType">
                                            </td>
                                            <td class="w-1">1</td>
                                            <td>FieldType</td>
                                            <td class="text-muted">Field Type</td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td>
                                                <div class="btn-group">
                                                    <button type="button" class="btn btn-light"><i class="ti ti-pencil"></i></button>
                                                    <button type="button" class="btn btn-light"><i class="ti ti-trash"></i></button>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="w-1">
                                                <input class="form-check-input m-0 align-middle" type="checkbox" aria-label="Select DocType DocType">
                                            </td>
                                            <td class="w-1">2</td>
                                            <td>DocType</td>
                                            <td class="text-muted">DocType</td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td>
                                                <div class="btn-group">
                                                    <button type="button" class="btn btn-light"><i class="ti ti-pencil"></i></button>
                                                    <button type="button" class="btn btn-light"><i class="ti ti-trash"></i></button>
                                                </div>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td class="w-1">
                                                <input class="form-check-input m-0 align-middle" type="checkbox" aria-label="Select DocType DocField">
                                            </td>
                                            <td class="w-1">3</td>
                                            <td>DocField</td>
                                            <td class="text-muted">DocField</td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td class="text-muted"><i class="ti ti-checkbox"></i></td>
                                            <td class="text-muted"><i class="ti ti-square"></i></td>
                                            <td>
                                                <div class="btn-group">
                                                    <button type="button" class="btn btn-light"><i class="ti ti-pencil"></i></button>
                                                    <button type="button" class="btn btn-light"><i class="ti ti-trash"></i></button>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>             
                </div>
            </div>
        </div>
    </div>
</div>